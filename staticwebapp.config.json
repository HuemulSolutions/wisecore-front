{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/_framework/*", 
      "/static/*",
      "/*.{png,jpg,jpeg,gif,ico,css,js,svg,woff,woff2,eot,ttf,otf,webp,avif,mp4,webm,ogg,mp3,wav,flac,aac,map,json}"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  }
}